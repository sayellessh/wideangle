body { 
	padding: 0px;
	margin: 0px;
}
#page-download, #page-home {
	padding: 0px;
	margin: 0px;
	
}
  .container{
	width:100%;
	height:100%;
	background:#fff;
	position:fixed;
	top: 0px;
	left: 0px;
	bottom: 0px;
	right: 0px;
	z-index: 99999;
}
    .panel{
        width:100%;
        bottom:100px;
        top:0px
        position:fixed;
        bottom:140px;     
    }
    #myIframe,#divFrame{
	    width: 100% !important;
	    height: 100% !important; 
	    position: static; 
	    padding: 0;
	    margin: 0;
    }
   #divFrame{
    width: 100%;
    white-space: nowrap;
    text-align: center;
    }
   #divFrame img{
    vertical-align: middle;
    max-height: 100%;
    max-width: 100%;
    
    }
    
   /* #myIframe img,#divFrame img{
    display: block;
    margin-left: auto;
    margin-right: auto 
    }*/
    
    .tray{
            clear:both;
            width:100%;
            height:100px;
            background:#333;
            margin-top:20px;
            padding:10px 0;
            position:fixed;
            bottom:0px;

            
        }
    
    .prev{
            height:20px;
            width:5%;
            text-align:center;
            position:absolute;
            right:90px;
            top:35%;
            z-index:99999;
            color:#fff;
            font-size:50px;
            font-weight:bold;

        }
        .next{
            height:20px;
            width:5%;
            text-align:center;
            position:absolute;
            right:20px;
            top:35%;
            z-index:99999;
            color:#fff;
            font-size:50px;
            font-weight:bold;
        }
    .item_div{
    	width:90%;
    	background:#595959;
    	height:100px;
    	margin:0 0% 0 5%;
        border-radius:5px;
        list-style: none;
    }
    .item_div li {
    	list-style: none;
    }
    .item_div li img {
    	max-width: 100%;
    }
    .comments_div{
    	float:left;
    	width:29%;
    	background:#fff;
        height:100px;
        background:#1589ff;
        border-radius:5px;
    
    }
    .item{
        width:100px;
        height:80px;
        background:#fff;
        margin:10px 0% 10px 5%;
        float:left;
        text-align:center;
    	line-height:80px;
    	color:#000;
    }
    /*tab*/
    ul, li, div {
        background: hsla(0, 0%, 0%, 0);
        border: 0;
        font-size: 100%;
        margin: 0;
        outline: 0;
        padding: 0;
        vertical-align: baseline;
        font: 13px/20px "Droid Sans", Arial, "Helvetica Neue", "Lucida Grande", sans-serif;
        text-shadow: 0 1px 0 hsl(0, 100%, 100%);
    }
    li {
        display: list-item;
        text-align: -webkit-match-parent;
    }
    .tabs-nav {
        list-style: none;
        margin: 0;
        padding: 0;
    	width:100%;
    	height:50px;
        border-bottom:#ddd solid 1px; 
    }
    .tabs-nav li:first-child{
    	border-right:#ddd solid 1px;
    }
    .tabs-nav li:first-child a {
    	
   
    }
    .tabs-nav .tab-active a {
        color: #1589ff !important;
        cursor: default;
        border-bottom:5px solid #1589ff;
    }
    .tabs-nav a {
        color: #333 !important;
        display: block;
        font-size: 16px;
        font-weight: bold;
        height: 45px;
        line-height: 54px;
        text-align: center;
        text-transform: uppercase;
        text-decoration:none;
    	border-bottom:5px solid #fff;     
    }
    .tabs-nav li:last-child a{
    	border-right:0;
    }
    .tabs-nav li {
        float: left;
        width:50%;
    	box-sizing:border-box;

    }
    .tabs-stage {
        border-top: 0;
        clear: both;
        margin-bottom: 20px;
        position: relative;
        top: -1px;
        width: 100%;
    }
    .tabs-stage p {
        margin: 0;
        padding: 20px;
        color:#000;
    }
    .top-div{
    	width:96%;
    	height:60px;
        background: #999;
        margin:20px auto;
    	
    }
    .bottom-div{
    	clear:both;
    	width:96%;
	    background:#ddd;
        margin:auto;
    	height:auto;
    }
    .bottom-div .left-sec{
    	width:50%;
    	float:left;
    	padding:20px 0;
    	
    }
   
    .top-div ul li{
    	border-left:#333 1px solid;
        float:left;
        list-style:none;
    	padding:20px 0;
        text-align:center;
        width:20%;
    	box-sizing:border-box;
    	font-size:14px;
	
    	
    }
    .top-div ul li:first-child{
    	border-left:0;
    }
   .like-div a.like{
	   background:url("../hidoctor/images/like.png") no-repeat;
       width:66px;
       height:28px;
       display:inline-block;
       margin-left:70px
	   
   }
   .like-div a.dislike{
	   background:url("../hidoctor/images/unlike.png") no-repeat;
       width:35px;
       height:28px;
       display:inline-block;;
       margin-left:20px
	   
   }
   .rate-div ul{
       margin-left:70px;
       margin-top:20px;
   }
   .rate-div ul li{
	  list-style:none;
   	  float:left;
   }
   .rate-div ul li a{
	   background:url("rating/empty-star.gif") no-repeat;
