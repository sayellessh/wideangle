/* menu section */
.menu{
    height:45px;
    width:100%;
    border-bottom:2px solid #000;
    background:#f2f2f2;
}
.search_sec{
    float:right;
    margin-right:20px;

}
.search_sec .ui-input-search, div.ui-input-text{
	 margin:5px 0 !important;
     background:#f2f2f2 url("../../hidoctor/images/Searchnew.png") no-repeat center right !important;
     border-radius:25px !important;
     width:150px !important;
     color:#000 !important;
     padding: 0 4.5em 0 1.5em !important;
}
.search_sec input{
    width:150px;
    border-radius:10px;
    height:32px;
    background:#f2f2f2;
    margin-right:20px;
    color:#000 !important;
    text-shadow:none !important;
}
.menu ul.slide_menu{
    float:right;
    list-style:none;
    margin-right:10px;
    height:45px;
}
.menu ul.slide_menu li{
    float:left;
    padding:10px 0 5px;
    border-bottom:#f2f2f2 3px solid;
}
.menu ul.slide_menu li a{
	float: left; 
    display:block;
    color:#626262;
    height:25px;
    font-size:14px;
    line-height:25px;
    padding:0px 10px;
    text-decoration:none;
    border-right:2px solid #b4b4b4;
}
.menu ul.slide_menu li:first-child a{
   border-left:2px solid #b4b4b4;
}
.menu ul.slide_menu li.active{
    border-bottom:#1c36fc 3px solid;
}
.menu ul.slide_menu li.active a{
    color:#1c36fc;
}
.menu .product_sec{
    float:left;
    width:450px;
    margin-left:50px;
}
.menu .product_sec label{
    color:#626262;
    line-height:45px;
    float:left;
    margin-right:10px;
}
.menu .select_box{
    float:left;
}
.menu .ui-select{
    width:250px;
    float:left;
}
.menu .ui-select .ui-btn{
    background:#fff;
    border:1px solid #D3D3D3 !important;
}
.menu .ui-btn-inner{
   padding: .4em 8px !important;
   font-size:14px !important;
}
.menu span.productSelect{
    color:#000 !important;

}
.menu .ui-select .ui-btn-text{
    text-align:left;
}

.menu .show{
    float:left;
    background:#0070D9;
    padding:10px;
    color:#fff;
    font-size:16px;
    line-height:13px;
    margin-top:6px;
    text-align:center;
    margin-left:10px;
    border-radius:3px;
}
.ui-body-a .ui-link:hover{
    color:#626262 !important;
