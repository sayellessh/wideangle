#story_modal_wrapper {
    background-color: rgba(0, 0, 0, 0.9);
    position: fixed;
    top: 0px;
    left: 0px;
    right: 0px;
    bottom: 0px;
    z-index: 9999999;
    display: none;
}

#story_modal {
    width: 300px;
    height: 192px;
    left: 50%;
    margin-left: -150px;
    top: 50%;
    margin-top: -96px;
    position: absolute;
    
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    background: #d9d9d9;
    border-radius: 4px;
    padding: 25px 0px 0px;
}
#story_modal.list_popup {
    height: 400px;
    top: 50%;
    margin-top: -200px;
    overflow-y: scroll;
}
#story_modal label {
    color: #000;
    font: bold 16px/32px Arial;
    display: inline-block;
    padding-left: 4%;
    padding-bottom: 15px;
    text-align: center;
}
#story_modal input {
    color: #000 !important;
    border: 1px solid #999;
    background: #fff;
    width: 92%;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    -webkit-appearance: none;
    
    padding: 0px 10px;
    height: 38px;
    font-size: 14px;
    box-shadow: none !important;
    -webkit-box-shadow: none !important;
    margin-left: 4%;
}

#story_modal .modal_action a {
    width: 50%;
    display: inline-block;
    float: left;
    font-size: 16px;
    line-height: 50px;
    color: #016FDF;
    text-align: center;
    text-decoration: none;
    border-right: 1px solid #b9b9b9;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
}
#story_modal .modal_action a:last-child {
    border-right: 0px;
}
#story_modal .modal_action {
    margin-top: 30px;
    border-top: 1px solid #b9b9b9;
}
#story_modal .modal_action:before,
#story_modal .modal_action:after {
    display: table;
    content: "";
}
#story_modal .modal_action:after {
    clear: both;
}

.menu_sub_list {
    display: none;
    width:244px;
    list-style:none;
    position:absolute;
    z-index:999999;
}
.menu_sub_list li a {
    text-decoration: none;
}
.list_popup .story_popup {
    width: 100%;
}
.list_popup .story_popup li {
    display: block;
    border-bottom: 1px solid #aaa;
}
.list_popup .story_popup li a {
    display: block;
    padding: 10px 15px;
    font: 16px/24px Arial;
    color: #333;
    text-decoration: none;
}
#asset_part p.warning_msg {
    font-size: 20px;
    line-height: 28px;
    padding: 20% 20px 0px;
    color: #844;
    text-align: center;
}
#asset_part p.info_msg,#asset_part p.success_msg,#story_part p.success_msg {
    width: 880px;
    margin: 0px auto;
    height: 24px;
    padding: 10px 0px;
